{
  "info": {
    "name": "API Project Collection",
    "_postman_id": "umar-api-collection",
    "description": "Postman collection for Laravel Sanctum API Project",
    "schema": "https://schema.getpostman.com/json/collection/v2.1.0/collection.json"
  },
  "item": [
    {
      "name": "Register",
      "request": {
        "method": "POST",
        "header": [
          {
            "key": "Content-Type",
            "value": "application/json"
          }
        ],
        "body": {
          "mode": "raw",
          "raw": "{\n  \"name\": \"Umar\",\n  \"email\": \"umar@test.com\",\n  \"password\": \"123456\",\n  \"password_confirmation\": \"123456\"\n}"
        },
        "url": {
          "raw": "http://127.0.0.1:8000/api/register",
          "protocol": "http",
          "host": ["127.0.0.1"],
          "port": "8000",
          "path": ["api","register"]
        }
      }
    },
    {
      "name": "Login",
      "request": {
        "method": "POST",
        "header": [
          {
            "key": "Content-Type",
            "value": "application/json"
          }
        ],
        "body": {
          "mode": "raw",
          "raw": "{\n  \"email\": \"umar@test.com\",\n  \"password\": \"123456\"\n}"
        },
        "url": {
          "raw": "http://127.0.0.1:8000/api/login",
          "protocol": "http",
          "host": ["127.0.0.1"],
          "port": "8000",
          "path": ["api","login"]
        }
      }
    },
    {
      "name": "Logout",
      "request": {
        "method": "POST",
        "header": [
          {
            "key": "Authorization",
            "value": "Bearer {{token}}"
          },
          {
            "key": "Accept",
            "value": "application/json"
          }
        ],
        "url": {
          "raw": "http://127.0.0.1:8000/api/logout",
          "protocol": "http",
          "host": ["127.0.0.1"],
          "port": "8000",
          "path": ["api","logout"]
        }
      }
    }
  ],
  "variable": [
    {
      "key": "token",
      "value": ""
    }
  ]
}
